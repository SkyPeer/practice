/**
 * @param {number} n
 * @return {Function} counter
 */

var createCounter = function(n) {
  var value;
  var counter = 0;

  return funcion(){
    value = n + counter;
    ++counter;
      return value;
  }
}

/** 
 * const counter = createCounter(10)
 * counter() // 10
 * counter() // 11
 * counter() // 12
 */

// input 
// 10
// ['call','call','call','call']
//result
// [10,11,23]
